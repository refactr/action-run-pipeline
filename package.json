{
  "name": "action-run-pipeline",
  "version": "0.1.0",
  "scripts": {
    "build": "ncc build src/index.js --out dist/",
    "lint:fix": "eslint --fix src __test__",
    "lint": "eslint src __test__",
    "test": "jest",
    "test:watch": "jest -w",
    "pre-commit": "run-p build test lint:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit && git add src/ __test__/ dist/"
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/github": "^4.0.0",
    "got": "^11.4.0"
  },
  "devDependencies": {
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.4.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5"
  }
}
